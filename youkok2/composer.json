{
  "name": "youkok2/youkok2",
  "description": "Utility website for students",
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "slim/slim": "3.12.1",
    "monolog/monolog": "1.24.0",
    "illuminate/database": "5.8.30",
    "illuminate/events": "5.8.30",
    "robmorgan/phinx": "0.10.8",
    "illuminate/cache": "5.8.30",
    "myclabs/php-enum": "^1.7",
    "ext-json": "*",
    "ext-redis": "*",
    "ext-fileinfo": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.4.2",
    "phpunit/phpunit": "8.2.5",
    "php-coveralls/php-coveralls": "1.1.0"
  },
  "autoload": {
    "psr-4": {
      "Youkok\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Youkok\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": [
      "@phpunit",
      "@phpcs",
      "@phpmd"
    ],
    "phpunit": "vendor/bin/phpunit",
    "phpcs": "vendor/bin/phpcs",
    "migrate": "vendor/bin/phinx migrate -c phinx/config.php"
  }
}
